    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.37.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "dependencies": {
    "cookie": "^0.4.1",
    "html2canvas": "^1.3.2",
    "marked": "^3.0.0",
    "monaco-editor": "^0.26.1",
    "split.js": "^1.6.5",
